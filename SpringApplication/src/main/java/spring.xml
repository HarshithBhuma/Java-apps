<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">


<beans>

	<bean id="David" class="com.ex.Employee">

		<property name="empId" value="123"></property>
		<property name="empName" value="David"></property>
		<property name="empAge" value="36"></property>
		<property name="location">
			<map>
				<entry key="1" value="Dallas"></entry>
				<entry key="2" value="Houston"></entry>
				<entry key="3" value="Illinois"></entry>

			</map>

		</property>

	</bean>

	<!-- <bean id = "David" class="com.ex.Employee"> <constructor-arg value 
		= "123"></constructor-arg> <constructor-arg value = "David"></constructor-arg> 
		<constructor-arg value = "36"></constructor-arg> </bean> -->

</beans>