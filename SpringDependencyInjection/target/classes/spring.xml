		<property name="name" value="Alice"></property>
	</bean>

	<!-- <bean id="Appointment" class="com.ex.Appointment"> <property name="Apollo"> 
		<ref bean="Alice" /> </property> </bean> -->

	<bean id="Appointment" class="com.ex.Appointment"
		autowire="byName">

	</bean>
</beans>